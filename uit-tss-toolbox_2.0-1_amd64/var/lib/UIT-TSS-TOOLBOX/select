#!/usr/bin/php
<?php

include('/var/lib/UIT-TSS-TOOLBOX/PDO-connect');

$stdin = fopen('php://stdin', 'r');
$line = trim(fgets(STDIN));
fclose($stdin);

$sql = $pdo->prepare($line);
$sql->execute();
$row = $sql->fetchAll(PDO::FETCH_COLUMN);

foreach ($row as $key => $value) {
        echo $value . PHP_EOL;
}
?>
